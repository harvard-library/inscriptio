<div class="call_number show-object">
  <% content_for :header do %><%= @call_number %><% end %>
  <dl>
	<dt>Long Name:</dt>
    <dd><%= @call_number.long_name %></dd>
    <dt>Description:</dt>
    <dd><%= @call_number.description %></dd>
    <dt>Floors:</dt>
    <dd><%= @call_number.floors.join(', ') %></dd>
    <% if @call_number.subject_area %>
    <dt>Subject Area:</dt>
    <dd><%= link_to @call_number.subject_area, subject_area_path(@call_number.subject_area.id) %></dd>
    <% end %>
  </dl>
</div>
