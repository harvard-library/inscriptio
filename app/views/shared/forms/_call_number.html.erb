<%= semantic_form_for(@call_number) do |f| %>
  <%= f.inputs do %>
    <%= f.input :call_number %>
	<%= f.input :long_name %>
    <%= f.input :description %>
    <%= f.input :subject_area, :required => true, :include_blank => false %>
    <%= f.input :floors, :as => :check_boxes %>
  <% end %>
  <%= f.actions %>
<% end %>
