<script type="text/javascript">
       bkLib.onDomLoaded(function() { nicEditors.allTextAreas() });
</script>

<%= semantic_form_for(@library) do |f| %>
  <%= f.inputs do %>
      <%= f.input :name %>
      <%= f.input :url %>
      <%= f.input :bcc %>
      <%= f.input :from %>
      <%= f.input :address_1 %>
      <%= f.input :address_2 %>
      <%= f.input :city %>
