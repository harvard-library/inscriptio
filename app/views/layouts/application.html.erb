<!DOCTYPE html>
<html>
  <head>
    <title>Inscriptio</title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag 'jquery.min', 'rails.js' %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <% if flash[:notice] %>
      <div class="notice"><%= flash[:notice] %></div>
    <% end %>

    <% if flash[:alert] %>
      <div class="alert"><%= flash[:alert] %></div>
    <% end %>

    <% if flash[:error] %>
      <div class="error"><%= flash[:error] %></div>
    <% end %>

    <%= yield %>

  </body>
</html>
