<div class="reservable_assets collection-list">
  <h1>Reservable Assets</h1>
  <div id="content-right">
  <% unless @reservable_assets.blank? %>
    <%= render :partial => 'shared/lines/reservable_asset', :collection => @reservable_assets %>
  <% else %>
    <h2>No reservable assets for this floor yet.</h2>
  <% end %>
  <% if current_user.admin? %>
    <h2 class="action-button"><%= link_to('New Reservable Asset', new_reservable_asset_path) %></h2>
  <% end %>  
</div>
</div>
